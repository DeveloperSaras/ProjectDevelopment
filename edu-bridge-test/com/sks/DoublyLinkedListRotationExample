package com.sks;

public class DoublyLinkedListRotationExample {

	public static void main(String[] args) {
	
		DoublyLinkedList list = new DoublyLinkedList();
		list.push(1);
		list.push(2);
		list.push(3);
		list.push(4);
		list.push(5);

		int rotateNoOfTimes = 3;

		System.out.println("Given linked list ");
		list.displayList(list.getHead());
		list.rotateList(rotateNoOfTimes);
		System.out.println();
		System.out.println("Linked list after rotation ");
		list.displayList(list.getHead());
	}

}
